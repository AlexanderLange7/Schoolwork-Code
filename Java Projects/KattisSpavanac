
/**
 *
 * @author Alex
 */
import java.util.Scanner;
public class main {
   
    public static void main(String [] args){
        Scanner input = new Scanner(System.in);
        int H = input.nextInt();
        int M = input.nextInt();
        
        if (M <45){
            if(H==0){
            H = 23;}
            else{
            H = H-1;
            }
            M = M+15;
        }
        else{
            M = M-45;
        }
     System.out.println(H +" "+ M);

    }
}
