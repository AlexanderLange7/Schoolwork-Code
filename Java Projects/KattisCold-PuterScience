
/**
 *
 * @author Alex
 */
import java.util.Scanner;

public class main {

    public static void main(String[] args) {
       Scanner input = new Scanner(System.in);
       int count = 0;
       int n = input.nextInt();
       for (int i = 0; i<n; i++){
       int I = input.nextInt();
       if(I <0){count++;}
       }
       
       System.out.println(count);
        
        
        
    }
}
