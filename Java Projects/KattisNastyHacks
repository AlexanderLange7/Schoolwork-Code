 * @author Alex
 */
import java.util.Scanner;

public class main {

    public static void main(String[] args) {
       Scanner runthrough = new Scanner(System.in);
       
       int n = runthrough.nextInt();
        for(int i = 0; i<n; i++){
            //Sins Advert
            int r = runthrough.nextInt();
            //With Advert
            int e = runthrough.nextInt();
            //Cost
            int c = runthrough.nextInt();
            
            //If the Revenue for advertising minus its cost if greater than sins advert
            if(e-r>c){
                System.out.println("advertise");
            }
            if(e-r<c){
                System.out.println("do not advertise");
            }
            if(e-r==c){
                System.out.println("does not matter");
            }
        }
        
        
    }
}
